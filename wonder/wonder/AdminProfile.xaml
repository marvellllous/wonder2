<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="wonder.AdminProfile">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="UserName" Placeholder="Enter name" />
            <Entry x:Name="Password" Placeholder="Enter Password" />
            <Button Text="Login" x:Name="AdmLogin" Clicked="AdmLogin_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>